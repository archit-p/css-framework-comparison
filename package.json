  },
  "devDependencies": {
    "bootstrap": "^4.5.2",
    "bulma": "^0.9.1",
    "foundation-sites": "^6.6.3",
    "gh-pages": "^3.1.0",
    "materialize-css": "^1.0.0",
    "milligram": "^1.4.1",
    "prettier": "2.1.2",
    "purecss": "^2.0.3",
    "shelljs": "^0.8.4",
